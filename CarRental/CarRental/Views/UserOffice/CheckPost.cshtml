
@{
    ViewBag.Title = "CheckPost";
}

@model List<WorkWithData.Models.Message>

<main>
    <div class="aside">
        @Html.Partial("PartialForUserOffice")
    </div>
    <article>
        @foreach (var item in Model)
         {
            <div class="PostBlock">
                <p>@Html.DisplayFor(m => item.Date)</p>
                <br />
                <p>@Html.DisplayFor(m => item.Text)</p>
            </div>
          }
    </article>
</main>
